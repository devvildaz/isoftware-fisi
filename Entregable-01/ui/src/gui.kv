#:import DirectorySearch components.DirectorySearch
#:import ImageButton components.ImageButton

FloatLayout:
  id: floatid
  BoxLayout:
    id: boxid
    orientation: 'vertical'
    canvas:
      Color:
        rgba: .0,.0,.0,1
      Rectangle:
        pos: self.pos
        size: self.size
    DirectorySearch:
    StackLayout:
      id: stackimages
      spacing: (10,10)
      padding: (10,10)
      canvas:
        Color:
          rgba: .6,.5,.5, 0
        Rectangle:
          pos: self.pos
          size: self.size
  
